<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="MissingDefaultResource">

    <android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/colorPrimary"
        android:padding="5dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#fff"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/user_image"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_alignParentStart="true"
                    android:layout_marginStart="10dp"
                    android:src="@drawable/ic_dashboard_black_24dp" />

                <LinearLayout
                    android:id="@+id/lin"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="13dp"
                    android:layout_toEndOf="@id/user_image"
                    android:gravity="end"
                    android:orientation="vertical">

                    <android.support.design.widget.TextInputEditText
                        android:id="@+id/project_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="#fff"

                        android:hint="@string/pressHere"
                        android:maxLines="2"
                        android:layout_gravity="start"
                        android:gravity="start"
                        android:textColor="@color/colorPrimary"
                        android:textColorHint="@color/colorPrimary"
                        android:textSize="16sp" />

                </LinearLayout>

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:visibility="gone">


                <Spinner
                    android:id="@+id/sp_proType"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_marginStart="13dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="5dp"
                    android:layout_marginBottom="5dp"
                    android:layout_toStartOf="@id/rrrr"
                    android:entries="@array/pro_type">

                </Spinner>


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:id="@+id/rrrr"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="20dp"
                    android:layout_marginBottom="5dp"
                    android:text="نوع المشروع"
                    android:textColor="#000"
                    android:textSize="18sp" />
            </RelativeLayout>

            <EditText
                android:id="@+id/proDetail"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:gravity="start"
                android:layout_margin="8dp"
                android:background="@drawable/rounded_corner_edit_text"

                android:hint="@string/projectDesc"
                android:lines="4"
                android:padding="10dp"
                android:textSize="16sp" />

            <RelativeLayout
                android:id="@+id/money"
                android:layout_width="match_parent"
                android:layout_height="30dp">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="25dp"
                    android:layout_centerVertical="true"
                    android:background="#568D96"
                    android:gravity="center_vertical" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentBottom="true"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="0dp"
                    android:layout_marginBottom="0dp"
                    android:textColor="@color/colorWhite"
                    android:layout_toEndOf="@+id/arrowMoney"

                    android:gravity="center_vertical"
                    android:text="@string/money"
                    android:textAlignment="center"
                    android:textSize="16sp" />

                <ImageView xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:id="@+id/arrowMoney"
                    android:layout_width="30dp"
                    android:layout_height="match_parent"
                    android:layout_alignParentTop="true"
                    android:layout_marginStart="10dp"
                    android:background="@android:color/transparent"
                    android:foregroundGravity="center"
                    android:src="@drawable/ic_keyboard_arrow_down_white_24dp" />
            </RelativeLayout>

            <LinearLayout
                android:id="@+id/moneySection"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    android:visibility="gone"
                    android:text="@string/proDuration"
                    android:textColor="#000"
                    android:textSize="18sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:visibility="gone">

                    <Spinner
                        android:id="@+id/sp_durationType"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:paddingStart="15dp"
                        android:spinnerMode="dialog"
                        style="@style/spinner_style_left"
                        android:entries="@array/durations">

                    </Spinner>

                    <EditText
                        android:id="@+id/ed_proDuration"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="end"
                        android:inputType="number"
                        android:paddingEnd="10dp"
                        android:text="0" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:paddingStart="10dp"
                        android:paddingEnd="10dp"
                        android:text="@string/from"
                        android:textColor="#000"
                        android:textSize="18sp" />
                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    android:gravity="start"
                    android:text="@string/moneyAmount"
                    android:textColor="#000"
                    android:textSize="16sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">


                    <!--<Spinner-->
                    <!--android:id="@+id/sp_initialCost"-->
                    <!--android:layout_width="wrap_content"-->
                    <!--android:layout_height="wrap_content"-->
                    <!--android:layout_margin="5dp"-->
                    <!--android:entries="@array/durations_time">-->

                    <!--</Spinner>-->


                    <EditText
                        android:id="@+id/ed_initialCost"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="start"
                        android:textSize="14sp"
                        android:inputType="number"
                        android:paddingStart="15dp"
                        android:hint="0" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:paddingStart="10dp"
                        android:paddingEnd="10dp"
                        android:text="@string/from"
                        android:textColor="#000"
                        android:textSize="18sp" />

                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    android:gravity="start"
                    android:text="@string/directCost"
                    android:textColor="#000"
                    android:textSize="16sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">


                    <Spinner
                        android:id="@+id/sp_directCost"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:paddingStart="20dp"
                        android:spinnerMode="dialog"
                        style="@style/spinner_style_left"
                        android:visibility="gone"
                        android:entries="@array/durations">

                    </Spinner>



                    <EditText
                        android:id="@+id/ed_directCost"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="start"
                        android:textSize="14sp"
                        android:inputType="number"
                        android:paddingStart="15dp"
                        android:hint="0" />


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:paddingStart="10dp"
                        android:paddingEnd="10dp"
                        android:text="@string/from"
                        android:textColor="#000"
                        android:textSize="18sp" />
                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    android:gravity="start"
                    android:text="@string/IndirectCost"
                    android:textColor="#000"
                    android:textSize="16sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">


                    <Spinner
                        android:id="@+id/sp_IndirectCost"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:paddingStart="20dp"
                        android:spinnerMode="dialog"
                        style="@style/spinner_style_left"
                        android:visibility="gone"
                        android:entries="@array/durations">

                    </Spinner>


                    <EditText
                        android:id="@+id/ed_IndirectCost"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="start"
                        android:textSize="14sp"
                        android:inputType="number"
                        android:paddingStart="15dp"
                        android:hint="0" />


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:paddingStart="10dp"
                        android:paddingEnd="10dp"
                        android:text="@string/from"
                        android:textColor="#000"
                        android:textSize="18sp" />
                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    android:gravity="start"
                    android:text="@string/totalCostExcpected"
                    android:textColor="#000"
                    android:textSize="16sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">


                    <Spinner
                        android:id="@+id/sp_totalCost"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:paddingStart="20dp"
                        android:spinnerMode="dialog"
                        style="@style/spinner_style_left"
                        android:visibility="gone"
                        android:entries="@array/durations">

                    </Spinner>



                    <EditText
                        android:id="@+id/ed_totalCost"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="start"
                        android:textSize="14sp"
                        android:enabled="false"
                        android:paddingStart="15dp"
                        android:text="0" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:paddingStart="10dp"
                        android:paddingEnd="10dp"
                        android:text="@string/from"
                        android:textColor="#000"
                        android:textSize="18sp" />
                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top"

                    android:gravity="top"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    android:visibility="gone"
                    android:text="@string/moneyType"
                    android:textColor="#000"
                    android:textSize="18sp" />

                <RadioGroup
                    android:id="@+id/moneyGroup"
                    android:layout_width="match_parent"
                    android:visibility="gone"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <RadioButton
                        android:id="@+id/day"
                        style="@style/Base.Widget.AppCompat.CompoundButton.CheckBox"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"

                        android:gravity="end|center_vertical"
                        android:text="@string/day"
                        app:buttonTint="@color/backgroundDarkGreen" />

                    <RadioButton
                        android:id="@+id/month"
                        style="@style/Base.Widget.AppCompat.CompoundButton.CheckBox"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"

                        android:gravity="end|center_vertical"
                        android:text="@string/month"
                        app:buttonTint="@color/backgroundDarkGreen" />

                    <RadioButton
                        android:id="@+id/year"
                        style="@style/Base.Widget.AppCompat.CompoundButton.CheckBox"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"

                        android:gravity="end|center_vertical"
                        android:text="@string/year"
                        app:buttonTint="@color/backgroundDarkGreen" />

                    <RadioButton
                        android:id="@+id/other"
                        style="@style/Base.Widget.AppCompat.CompoundButton.CheckBox"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:checked="true"

                        android:gravity="end|center_vertical"
                        android:text="@string/anotherKind"
                        app:buttonTint="@color/backgroundDarkGreen" />

                </RadioGroup>

                <!--<EditText-->
                <!--android:id="@+id/moneyDesc"-->
                <!--android:layout_width="match_parent"-->
                <!--android:layout_height="match_parent"-->
                <!--android:layout_gravity="right"-->
                <!--android:layout_margin="8dp"-->
                <!--android:background="@drawable/rounded_corner_edit_text"-->
                <!---->
                <!--android:gravity="right"-->
                <!--android:hint="@string/moneyDetail"-->
                <!--android:lines="1"-->
                <!--android:padding="10dp" />-->

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"

                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    android:gravity="start"
                    android:text="@string/moneyExpected"
                    android:textColor="#000"
                    android:textSize="16sp" />


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">


                    <Spinner
                        android:id="@+id/sp_profitMoney"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:paddingStart="20dp"
                        android:spinnerMode="dialog"
                        style="@style/spinner_style_left"
                        android:visibility="gone"
                        android:entries="@array/durations">

                    </Spinner>



                    <EditText
                        android:id="@+id/ed_profitMoney"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="start"
                        android:textSize="14sp"
                        android:inputType="number"
                        android:paddingStart="15dp"
                        android:hint="0" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:paddingStart="10dp"
                        android:paddingEnd="10dp"
                        android:text="@string/from"
                        android:textColor="#000"
                        android:textSize="18sp" />
                </LinearLayout>

            </LinearLayout>

            <RelativeLayout
                android:id="@+id/contributors"
                android:layout_width="match_parent"
                android:layout_height="30dp">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="25dp"
                    android:layout_centerVertical="true"
                    android:background="#568D96"
                    android:gravity="center_vertical" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentBottom="true"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="0dp"
                    android:layout_marginBottom="0dp"
                    android:textColor="@color/colorWhite"
                    android:layout_toEndOf="@+id/arrowContributors"

                    android:gravity="center_vertical"
                    android:text="@string/contributors"
                    android:textAlignment="center"
                    android:textSize="16sp" />

                <ImageView xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:id="@+id/arrowContributors"
                    android:layout_width="30dp"
                    android:layout_height="match_parent"
                    android:layout_alignParentTop="true"
                    android:layout_marginStart="10dp"
                    android:background="@android:color/transparent"
                    android:foregroundGravity="center"
                    android:src="@drawable/ic_keyboard_arrow_down_white_24dp" />
            </RelativeLayout>

            <LinearLayout
                android:id="@+id/contributorsSection"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="10dp"
                    android:layout_marginStart="10dp"

                    android:gravity="start"
                    android:text="@string/gender"
                    android:textColor="#000"
                    android:textSize="16sp" />

                <RadioGroup
                    android:id="@+id/genderGroup"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="5dp">

                    <RadioButton
                        android:id="@+id/both"
                        style="@style/Base.Widget.AppCompat.CompoundButton.CheckBox"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:checked="true"

                        android:gravity="start|center_vertical"
                        android:text="@string/both"
                        android:textSize="14sp"
                        app:buttonTint="@color/backgroundDarkGreen" />

                    <RadioButton
                        android:id="@+id/male"
                        style="@style/Base.Widget.AppCompat.CompoundButton.CheckBox"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"

                        android:gravity="start|center_vertical"
                        android:text="@string/male"
                        android:textSize="14sp"
                        app:buttonTint="@color/backgroundDarkGreen" />

                    <RadioButton
                        android:id="@+id/female"
                        style="@style/Base.Widget.AppCompat.CompoundButton.CheckBox"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"

                        android:gravity="start|center_vertical"
                        android:text="@string/female"
                        android:textSize="14sp"
                        app:buttonTint="@color/backgroundDarkGreen" />

                </RadioGroup>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="10dp"
                    android:layout_marginStart="10dp"
                    android:gravity="start"

                    android:text="@string/numContributors"
                    android:textColor="#000"
                    android:textSize="16sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">



                    <EditText
                        android:id="@+id/ed_contributers"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="start"
                        android:inputType="number"
                        android:textSize="14sp"
                        android:paddingStart="15dp"
                        android:text="0" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:paddingStart="10dp"
                        android:paddingEnd="10dp"
                        android:text="@string/from"
                        android:textColor="#000"
                        android:textSize="18sp" />
                </LinearLayout>

            </LinearLayout>


            <RelativeLayout
                android:id="@+id/place"
                android:layout_width="match_parent"
                android:layout_height="30dp">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="25dp"
                    android:layout_centerVertical="true"
                    android:background="#568D96"
                    android:gravity="center_vertical" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentBottom="true"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="0dp"
                    android:layout_marginBottom="0dp"
                    android:textColor="@color/colorWhite"
                    android:layout_toEndOf="@+id/arrowPlace"

                    android:gravity="center_vertical"
                    android:text="@string/place"
                    android:textAlignment="center"
                    android:textSize="16sp" />


                <ImageView xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:id="@+id/arrowPlace"
                    android:layout_width="30dp"
                    android:layout_height="match_parent"
                    android:layout_alignParentTop="true"
                    android:layout_marginStart="10dp"
                    android:background="@android:color/transparent"
                    android:foregroundGravity="center"
                    android:src="@drawable/ic_keyboard_arrow_down_white_24dp" />
            </RelativeLayout>

            <LinearLayout
                android:id="@+id/placeSection"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="15dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:visibility="gone">

                    <RadioGroup
                        android:id="@+id/placeGroup"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <RadioButton
                            android:id="@+id/no"
                            style="@style/Base.Widget.AppCompat.CompoundButton.CheckBox"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"

                            android:gravity="end|center_vertical"
                            android:text="@string/no"
                            app:buttonTint="@color/backgroundDarkGreen" />

                        <RadioButton
                            android:id="@+id/yes"
                            style="@style/Base.Widget.AppCompat.CompoundButton.CheckBox"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:checked="true"

                            android:gravity="end|center_vertical"
                            android:text="@string/yes"
                            app:buttonTint="@color/backgroundDarkGreen" />

                    </RadioGroup>

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="1"

                        android:gravity="center"
                        android:text="@string/neddplace"
                        android:textColor="#000"
                        android:textSize="16sp" />
                </LinearLayout>


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    android:gravity="start"
                    android:text="@string/whatYouHave"
                    android:textColor="#000"
                    android:textSize="18sp" />

                <EditText
                    android:id="@+id/ed_whatYouHave"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="start"
                    android:gravity="start"
                    android:layout_margin="8dp"
                    android:textSize="14sp"
                    android:paddingStart="15dp"
                    android:background="@drawable/rounded_corner_edit_text"

                    android:hint="@string/whatYouHaveDesc"
                    android:lines="5"
                    android:padding="10dp" />


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"

                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    android:gravity="start"
                    android:text="@string/whatYouHaveNot"
                    android:textColor="#000"
                    android:textSize="18sp" />

                <EditText
                    android:id="@+id/ed_whatYouHaveNot"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="start"
                    android:gravity="start"
                    android:layout_margin="8dp"
                    android:paddingStart="15dp"
                    android:textSize="14sp"
                    android:background="@drawable/rounded_corner_edit_text"

                    android:hint="@string/whatYouHaveNotDesc"
                    android:lines="5"
                    android:padding="10dp" />

            </LinearLayout>

        </LinearLayout>

    </android.support.constraint.ConstraintLayout>
</ScrollView>